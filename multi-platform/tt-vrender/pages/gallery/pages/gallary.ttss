.container {
  width: 100%;
  height: 500rpx;
  position: relative;
}

.draw {
  width: 100%;
  height: 500rpx;
  display: block;
  margin: 0px;
}

.backup {
  opacity: 0;
  visibility: hidden;
  display: none;
}

.interactive {
  width: 100%;
  height: 500rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  pointer-events: none;
}
